{
  "name": "resume-build-api-exp",
  "version": "1.0.0",
  "description": "Resume Builder Handling RESTful API",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production && npm run server:prod",
    "server:prod": "node dist/app.js",
    "build": "babel app.js --out-dir build",
    "clean": "rimraf dis",
    "server": "cross-env DEBUG=app nodemon --exec babel-node app.js",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "prod": "npm-run-all clean build",
    "format": "prettier --write \"./**/*.js\""
  },
  "keywords": [],
  "author": "Sophat Chhay",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.10.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  }
}
